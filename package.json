{
  "name": "fqb",
  "version": "2.0.0",
  "description": "Facebook Graph API query builder for JavaScript",
  "main": "dist/fqb.cjs.js",
  "module": "dist/fqb.esm.js",
  "browser": "dist/fqb.umd.js",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "coverage": "codecov",
    "lint": "eslint \"{src,test}/**/*.ts\""
  },
  "files": [
    "index.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chunkai1312/fqb.git"
  },
  "keywords": [
    "fqb",
    "facebook",
    "fb",
    "graph",
    "api",
    "query",
    "builder"
  ],
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chunkai1312/fqb/issues"
  },
  "homepage": "https://github.com/chunkai1312/fqb#readme",
  "dependencies": {
    "create-hmac": "^1.1.4",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@types/create-hmac": "^1.1.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/qs": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "rollup": "1.1.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
